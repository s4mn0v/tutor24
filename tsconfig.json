{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "types": ["node", "bcrypt", "bcryptjs", "jsonwebtoken", "@prisma/client"],
    "typeRoots": [
      "./node_modules/@types"
    ]
  }
}
